{
  "name": "usabilitydynamics.com/wp-repository",
  "version": "0.1.0",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/UsabilityDynamics/wp-repository",
  "keywords": [],
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "source",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    },
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.3",
    "composer/installers": "~1.0",
    "usabilitydynamics/github-repo-updater": "0.2.2",
    "usabilitydynamics/lib-settings": "0.2.2"
  },
  "require-dev": {},
